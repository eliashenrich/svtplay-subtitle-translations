{
    "name": "SVT Play Translator",
    "description": "Translates SVT Play video subtitles",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "settings-ui/popup.html",
        "default_icon": {
            "16": "/assets/icon-16.png",
            "32": "/assets/icon-32.png",
            "48": "/assets/icon-48.png",
            "128": "/assets/icon-128.png"
          }
    },
    "icons": {
        "16": "/assets/icon-16.png",
        "32": "/assets/icon-32.png",
        "48": "/assets/icon-48.png",
        "128": "/assets/icon-128.png"
      }
  }